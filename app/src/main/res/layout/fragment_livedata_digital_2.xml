<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="100dp"
    tools:ignore="spusage">

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital1"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="90dp"
        android:background="@drawable/wheel3"
        app:sv_maxSpeed="0"
        app:sv_minSpeed="0"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="33dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <TextView
        android:id="@+id/digitalTextFirst1"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignStart="@id/digital1"
        android:layout_marginStart="25dp"
        android:layout_marginTop="108dp"
        android:letterSpacing="0.7"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/colorWhite"
        android:textSize="60dp" />

    <TextView
        android:id="@+id/digitalTextSecond1"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignStart="@id/digital1"
        android:layout_marginStart="90dp"
        android:layout_marginTop="108dp"
        android:letterSpacing="0.7"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/colorWhite"
        android:textSize="60dp" />

    <TextView
        android:id="@+id/digitalTextThird1"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignStart="@id/digital1"
        android:layout_marginStart="155dp"
        android:layout_marginTop="108dp"
        android:letterSpacing="0.7"
        android:text="0"
        android:textAlignment="center"
        android:textColor="@color/colorWhite"
        android:textSize="60dp" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital2"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@id/digital1"
        android:layout_alignTop="@id/digital1"
        android:layout_marginStart="210dp"
        android:background="@drawable/wheelback4"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="23dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <TextView
        android:id="@+id/digitalText2"
        android:layout_width="210dp"
        android:layout_height="100dp"
        android:layout_alignStart="@id/digital2"
        android:layout_marginStart="12dp"
        android:layout_marginTop="108dp"
        android:letterSpacing=".5"
        android:text=""
        android:textAlignment="center"
        android:textColor="@color/colorWhite"
        android:textSize="60dp" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital3"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@id/digital2"
        android:layout_alignTop="@id/digital1"
        android:layout_marginStart="210dp"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital4"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@+id/digital3"
        android:layout_alignTop="@id/digital1"
        android:layout_marginStart="210dp"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital5"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignParentStart="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="240dp"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital6"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@+id/digital2"
        android:layout_alignTop="@+id/digital5"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital7"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@+id/digital3"
        android:layout_alignTop="@+id/digital5"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />

    <com.github.anastr.speedviewlib.ProgressiveGauge
        android:id="@+id/digital8"
        android:layout_width="220dp"
        android:layout_height="166dp"
        android:layout_alignStart="@+id/digital4"
        android:layout_alignTop="@+id/digital5"
        android:background="@drawable/wheel3"
        app:sv_speedTextColor="@color/colorWhite"
        app:sv_speedTextFormat="INTEGER"
        app:sv_speedTextPadding="30dp"
        app:sv_speedTextPosition="LEFT"
        app:sv_speedTextSize="0dp"
        app:sv_speedometerBackColor="@color/transparent"
        app:sv_unitTextSize="0dp"
        app:sv_unitUnderSpeedText="false"
        app:sv_withTremble="false" />


</RelativeLayout>